@inherits LucidPage<IList<DesignDto>>
@using Lucid.Web.App.Planning
@{
    SetMenuPage(title: "Designs");
}

<div class="main-content">
    <ul id="designList">
        @foreach (var design in Model)
        {
            <li>Design: @design.Name</li>
        }
    </ul>
</div>

<div>
    @Html.LinkButtonModal("Start Design", Actions.StartDesign())
</div>
