@inherits System.Web.Mvc.WebViewPage<StartDesignModel>
@using System.Web.Mvc.Html
@using Demo.Web.Utility.Html
@using Demo.Web.App.Planning
<!DOCTYPE html>
<html>
<head>
    <title>Start Design</title>
    <meta charset="utf-8" />
</head>
<body>
    <div>Start design</div>

    @Html.ValidationSummary(true)
    @using (var form = Html.Form(Model.Cmd))
    {
        <div>
            @form.Html.LabelFor(c => c.Name, "Design Name")
            @form.Html.TextBoxFor(c => c.Name)
            @form.Html.ValidationMessageFor(c => c.Name)
        </div>

        <input type="submit" value="Start Design"/>
    }
</body>
</html>