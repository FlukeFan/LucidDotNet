@inherits LucidPage<GenerateModel>
@using Lucid.Web.App.Generation
@{
    SetLayout(title: "Generate Project");
}
<div>Generate Project</div>

@Html.ValidationSummary(true)
@using (var form = Html.FormFor(Model.Cmd).Begin())
{
    <div>
        @form.Html.LabelFor(c => c.Name, "Project Name")
        @form.Html.TextBoxFor(c => c.Name)
        @form.Html.ValidationMessageFor(c => c.Name)
    </div>

    <input type="submit" value="Generate Project" data-nopjax="true" />
}
