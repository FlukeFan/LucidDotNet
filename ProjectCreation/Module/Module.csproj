<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net461</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="System.IO.Compression" />
  </ItemGroup>

  <Target Name="ZipProject" BeforeTargets="Build">
    <MSBuild Projects="..\Zip\Zip.proj" />
    <CreateItem Include="Project.zip">
      <Output ItemName="EmbeddedResource" TaskParameter="Include" />
    </CreateItem>
  </Target>

</Project>
